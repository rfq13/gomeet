# API Configuration
VITE_API_URL=https://api-meet.filosofine.com

# LiveKit Configuration
VITE_LIVEKIT_URL=wss://livekit.filosofine.com
VITE_LIVEKIT_API_KEY=devkey
VITE_LIVEKIT_API_SECRET=secret

# TURN Server Configuration for WebRTC NAT Traversal
VITE_TURN_URL=turn:your-turn-server.com:3478
VITE_TURN_USERNAME=your-username
VITE_TURN_CREDENTIAL=your-credential

# WebRTC Configuration
VITE_WEBRTC_STUN_SERVERS=stun:stun.l.google.com:19302,stun:stun1.l.google.com:19302
VITE_WEBRTC_ICE_CANDIDATE_POOL_SIZE=10
VITE_WEBRTC_BANDWIDTH_LIMIT=2000

# Adaptive Bitrate Streaming Configuration
VITE_ADAPTIVE_BITRATE_ENABLED=true
VITE_VIDEO_QUALITY_THRESHOLD_HIGH=1000000
VITE_VIDEO_QUALITY_THRESHOLD_MEDIUM=500000
VITE_VIDEO_QUALITY_THRESHOLD_LOW=200000

# SFU Configuration
VITE_SFU_ENABLED=true
VITE_SFU_SIMULCAST_LAYERS=3
VITE_SFU_MAX_PARTICIPANTS=50

# Connection Quality Monitoring
VITE_QUALITY_MONITORING_ENABLED=true
VITE_QUALITY_MONITORING_INTERVAL=5000
VITE_CONNECTION_TIMEOUT=30000

# Application Configuration
VITE_APP_NAME=GoMeet
VITE_APP_VERSION=1.0.0